{
  "name": "Backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "docker:build": "docker compose build --no-cache",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:down:vol": "docker compose down -v",
    "docker:logs:api": "docker compose logs -f api",
    "db:generate": "docker exec -it finflow_api pnpm prisma generate",
    "db:migrate:dev": "docker exec -it finflow_api pnpm prisma migrate dev",
    "db:migrate:deploy": "docker exec -it finflow_api pnpm prisma migrate deploy",
    "db:seed": "docker exec -it finflow_api pnpm ts-node prisma/seed.ts",
    "mysql:shell": "docker exec -it finflow_mysql mysql -uvini -pvini123 finflow_db",
    "mysql:grant": "docker exec -it finflow_mysql mysql -uroot -proot -e \"GRANT ALL PRIVILEGES ON *.* TO 'vini'@'%' WITH GRANT OPTION; FLUSH PRIVILEGES;\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.2",
    "prisma": "^6.13.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
